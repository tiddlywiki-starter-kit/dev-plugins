{
	"directories": [
		{
			"path": "templates",
			"filesRegExp": "^.*\\.vue$",
			"searchSubdirectories": true,
			"fields": {
				"title": {
					"source": "filename",
					"prefix": "$:/plugins/oeyoews/neotw-filetree/templates/"
				},
				"hide-body": "yes"
			}
		},
		{
			"path": "plugins",
			"filesRegExp": "^.*\\.js$",
			"searchSubdirectories": true,
			"fields": {
				"title": {
					"source": "filename",
					"prefix": "$:/plugins/oeyoews/neotw-filetree/plugins/"
				},
				"module-type": "library",
				"type": "application/javascript"
			}
		},
		{
			"path": "components",
			"filesRegExp": "^.*\\.js$",
			"searchSubdirectories": true,
			"fields": {
				"title": {
					"source": "filename",
					"prefix": "$:/plugins/oeyoews/neotw-filetree/components/"
				},
				"module-type": "library",
				"type": "application/javascript"
			}
		}
	]
}